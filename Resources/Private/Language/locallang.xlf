		<body>
			<!-- Site Configuration -->
			<trans-unit id="site.configuration.tab">
				<source>POWER CAPTCHA</source>
			</trans-unit>
			<trans-unit id="site.configuration.api_key">
				<source>API Key (mandatory)</source>
			</trans-unit>
			<trans-unit id="site.configuration.api_key.description">
				<source>Enter your POWER CAPTCHA API Key. You can find your API Key in the API Key Management on power-captcha.com.</source>
			</trans-unit>
			<trans-unit id="site.configuration.secret_key">
				<source>Secret Key (mandatory)</source>
			</trans-unit>
			<trans-unit id="site.configuration.secret_key.description">
				<source>Enter your POWER CAPTCHA Secret Key. You can find your Secret Key in the API Key Management  on power-captcha.com.</source>
			</trans-unit>
			<trans-unit id="site.configuration.endpoint_base_url">
				<source>Endpoint base URL (optional)</source>
			</trans-unit>
			<trans-unit id="site.configuration.endpoint_base_url.description">
				<source>Only needed if you have an on-premises version with self-hosted POWER CAPTCHA endpoint.</source>
			</trans-unit>
			<trans-unit id="site.configuration.javascript_base_url">
				<source>JavaScript base URL (optional)</source>
			</trans-unit>
			<trans-unit id="site.configuration.javascript_base_url.description">
				<source>Only needed if you have an on-premises version with self-hosted POWER CAPTCHA JavaScript.</source>
			</trans-unit>

			<!-- Frontend -->
			<trans-unit id="message.blocked">
				<source>Submission of the form was blocked by POWER CAPTCHA. Please try again later.</source>
			</trans-unit>
			<trans-unit id="message.disabled_missing_configuration">
				<source>Warning: POWER CAPTCHA is disabled due to missing configuration! Please configure EXT:power_captcha or remove the field from this form.</source>
			</trans-unit>

			<!-- Error Codes -->
			<trans-unit id="message.error.no_token_field">
				<source>The form does not contain a token field.</source>
			</trans-unit>
			<trans-unit id="message.error.missing_token">
				<source>The token is empty.</source>
			</trans-unit>
			<trans-unit id="message.error.token_not_verified">
				<source>The token has not been verified.</source>
			</trans-unit>
			<trans-unit id="message.error.invalid_secret">
				<source>The secret key is missing or invalid.</source>
			</trans-unit>
			<trans-unit id="message.error.invalid_token">
				<source>The token is invalid.</source>
			</trans-unit>
			<trans-unit id="message.error.api_error">
				<source>Error connecting to the API. API Key maybe invalid.</source>
			</trans-unit>
		</body>
