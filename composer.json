{
    "name": "power-captcha/integration-typo3",
    "description": "POWER CAPTCHA protects your TYPO3 forms against bots and unauthorized persons. GDPR compliant!",
    "type": "typo3-cms-extension",
    "homepage": "https://power-captcha.com/en",
    "license": [
        "GPL-2.0-or-later"
    ],
    "require": {
        "typo3/cms-core": "^11.5 || ^12.4"
    },
    "suggest": {
		"in2code/powermail": "Support for EXT:powermail",
		"typo3/cms-form": "Support for EXT:form"
	},
    "autoload": {
        "psr-4": {
            "PowerCaptcha\\Typo3\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "power_captcha"
        }
    }
}